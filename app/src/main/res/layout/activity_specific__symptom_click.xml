<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.brittany.hcd.Specific_Symptom_click"
    android:orientation="vertical">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#40b953"
        android:layout_weight="0.41">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="60dp"
            android:text="Symptom for "
            android:id="@+id/textView"
            android:layout_alignParentTop="true"
            android:layout_alignParentLeft="true"
            android:layout_alignParentStart="true"
            android:background="#40b953"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:textAlignment="center"
            android:paddingTop="20dp"
            android:layout_marginLeft="50dp" />
        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:text="'date'"
            android:id="@+id/date"
            android:layout_row="0"
            android:layout_column="2" />


    </GridLayout>
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#85349d"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="5dp">


        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton"
            android:src="@drawable/edit_workout"
            android:layout_row="0"
            android:layout_column="0"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Head"
            android:id="@+id/textView2"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton12"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="5"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp" />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#85349d"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="5dp">


        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:background="@null"
            android:id="@+id/imageButton2"
            android:layout_row="0"
            android:layout_column="0"
            android:src="@drawable/edit_workout"
            android:onClick="Pushup_clicked"
            android:scaleType="fitCenter"
            android:paddingBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Shoulder"
            android:id="@+id/textView3"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton13"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="7"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp" />
    </GridLayout>
    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#85349d"
        android:paddingTop="20dp"
        android:paddingLeft="20dp"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="5dp">


        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton3"
            android:layout_row="0"
            android:src="@drawable/edit_workout"
            android:layout_column="0"
            android:onClick="Heartrate_clicked"
            android:background="@null"
            android:scaleType="fitCenter"
            android:paddingBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Knees"
            android:id="@+id/textView4"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />
        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton14"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="7"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp" />
    </GridLayout>

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_row="2"
        android:layout_column="2"
        android:background="#85349d"
        android:paddingTop="20dp"
        android:paddingLeft="20dp">

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton17"
            android:src="@drawable/edit_workout"
            android:layout_row="0"
            android:layout_column="0"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Toes"
            android:id="@+id/Date_textview"
            android:textSize="12pt"
            android:textColor="#ffffff"
            android:layout_row="0"
            android:layout_column="1"
            android:paddingLeft="40dp" />

        <ImageButton
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:id="@+id/imageButton16"
            android:src="@drawable/delete_workout"
            android:layout_row="0"
            android:layout_column="5"
            android:scaleType="fitCenter"
            android:background="@null"
            android:paddingBottom="10dp"
            android:layout_marginRight="40dp" />
    </GridLayout>
    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text=""
        android:id="@+id/blank"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:background="#40b953"
        android:textSize="12pt"
        android:textColor="#ffffff"
        android:paddingLeft="0dp"
        android:layout_weight="0.53"
        android:layout_marginTop="5dp" />

</LinearLayout>
