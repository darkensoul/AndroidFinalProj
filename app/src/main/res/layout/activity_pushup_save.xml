<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.brittany.hcd.Pushup_save"
    android:orientation="vertical"
    android:background="#2ca5ee">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Push-Up Counter"
        android:id="@+id/textView5"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18pt"
        android:textColor="#ffffff"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="40dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Your total Pushup: "
        android:id="@+id/textView6"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18pt"
        android:textColor="#ffffff"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="30dp" />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/textView_goalSave"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="30pt"
        android:textColor="#000000"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="150dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Has been saved! "
        android:id="@+id/textView_dbentry"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:textSize="18pt"
        android:textColor="#ffffff"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="35dp" />
</LinearLayout>
