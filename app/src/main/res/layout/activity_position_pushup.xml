<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.brittany.hcd.Position_pushup"
    android:background="#2ca5ee">


    <ImageButton

        android:id="@+id/imageButton6"

        android:layout_alignParentLeft="false"
        android:layout_alignParentStart="false"
        android:src="@drawable/push"
        android:layout_height="230dp"
        android:layout_width="230dp"
        android:layout_alignParentBottom="false"
        android:onClick="Counter_display_clicked"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="70dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Push-Up Counter"
        android:id="@+id/textView10"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:textSize="35dp"
        android:textColor="#ffffff"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Get Ready!"
        android:id="@+id/textView11"
        android:layout_below="@+id/textView10"
        android:textSize="30dp"
        android:textColor="#ffffff"
        android:layout_alignLeft="@+id/imageButton6"
        android:layout_alignStart="@+id/imageButton6"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="40dp" />

    <TextView
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="Place your chin near the circle when doing a repetition."
        android:id="@+id/textView12"
        android:layout_below="@+id/imageButton6"
        android:textSize="30dp"
        android:textColor="#ffffff"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="30dp"
        android:layout_marginLeft="30dp" />
</RelativeLayout>
